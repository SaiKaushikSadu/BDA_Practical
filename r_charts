v<-c(23, 45, 10, 34, 89, 20, 67, 99)
print(v)
asc<-sort(v)
print(asc)
des<-sort(v,TRUE)
print(des)
des<-sort(v,descending=TRUE)
des<-sort(v,decreasing=TRUE)
print(des)
sub1<-subset(v,v<50)
print(sub1)
sub2<-v[v<80]
print(sub2)
barplot(v,col="red",main="bar plot",xlab="x",ylab="y")
df1<-data.frame(
id=c(1,2,3,4)
)
df2<-data.frame(
id=c(6,7,8,9)
)
df3<-rbind(df1,df2)
print(df3)
print(str(df3))
print(summary(df3))
print(str(df3))
df_col<-df3$id
print(df_col)
