x <- c(1, 2, 3, 4, 5)
y <- c(2, 4, 6, 8, 10)
plot(x,y,main="Plot1",col="red",xlab="X",ylab="Y")
symbols(x,y,main="Plot2",col="red",xlab="X",ylab="Y",inches=0.5,circles=y)
barplot(y,main="Plot3",col="blue",xlab="X",ylab="Y",names.arg=x)
dotchart(y,labels=x,main="Plot4",col="green",xlab="X",ylab="Y")
barplot(y,main="Plot5",col="blue",xlab="X",ylab="Y",names.arg=x)
hist(y,main="Plot5",col="blue",xlab="X",ylab="Y",xlim=c(0,10),ylim=c(0,10),breaks=2)
boxplot(y,main="Plot6",ylab="Y",xlab="X")
pie(y,main="Plot7")
